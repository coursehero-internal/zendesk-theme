<div class="customerSupportTopContainer">
  <div class="container">
    <h1 class="hero-2 text-center pt-4 pb-1">
      <span>Customer Support</span>
    </h1>
    <p class="small text-center pb-1">
      Need help with Course Hero? Browse helpful resources below or  	
      <a href="#" onclick="toggleTicketForm()">
         contact us here.
      </a>
    </p>
    <div class="ch_h-search-form_container pt-3 pb-3">
          {{search instant=true placeholder='Search our FAQs'}}
    </div>  
  </div>
</div>
<div class="container">
  <main class="support_body_container">
    <div id="dynamic_generation" class="grid">
      <ul class="category-list">
        {{#each categories}}
          <li><a href="{{url}}">{{name}}</a></li>
        {{/each}}
        {{pagination}}
      </ul>
    </div>
    
 	</main>
</div>
