<script>
  smoothScroll.init();

  $(".support_tutorial_link").click(function(e){
    $("#support_tutorial_video").attr("src", $(this).attr("data-href"));
    $(".support_tutorial_link").removeClass("active");
    $(this).addClass("active");
    e.preventDefault();
  });

  enquire.register("screen and (min-width:768px)", {
    match : function() {
      $(".support_tutorial_carousel").owlCarousel({
        items : 4,
        itemsTablet: [768,3],
        pagination: false,
        navigation: true,
        navigationText : [
          "<i class='fa fa-angle-left'></i>",
          "<i class='fa fa-angle-right'></i>"
        ],
        });
      },
    unmatch: function(){
    $(".support_tutorial_carousel").data('owlCarousel').destroy();
    }
  });

  $(document).ready(function(){
  	homepageGeneration();
    $(".support_tutorial_videos").fitVids();
    $(".support_tutorial_link.phone").magnificPopup({
      type: 'iframe',
      mainClass: 'mfp-fade',
      removalDelay: 160,
      preloader: false,
      fixedContentPos: false
    });
  });
</script>

<script type='text/javascript'>
  $(window).on('hashchange', function (e) {
  if (window.location.hash === '#/contact-us') {
    toggleTicketForm();
	  window.location.hash = '';
    }
});

</script>

